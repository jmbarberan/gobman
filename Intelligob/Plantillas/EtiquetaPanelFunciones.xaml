<UserControl x:Class="Intelligob.Escritorio.Plantillas.EtiquetaPanelFunciones"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    <Grid>
        <Border MinHeight="36" MinWidth="36" BorderThickness="0" CornerRadius="0,6,6,0" BorderBrush="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}" Background="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}"/>
        <Image Name="imgIcono" Source="menuBlanco.png" Height="20" Width="24" VerticalAlignment="Center" HorizontalAlignment="Center" RenderTransformOrigin="0.5,0.5" Margin="0"/>
    </Grid>
</UserControl>
