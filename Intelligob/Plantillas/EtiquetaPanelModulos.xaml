<UserControl x:Class="Intelligob.Escritorio.Plantillas.EtiquetaPanelModulos"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    <Grid>
        <Border MinHeight="36" MinWidth="36" BorderThickness="0" CornerRadius="6,0,0,6" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" Background="{DynamicResource {x:Static SystemColors.AppWorkspaceBrushKey}}"/>
        <Image Name="imgIcono" Source="corchetes.png" Height="32" Width="32" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</UserControl>
